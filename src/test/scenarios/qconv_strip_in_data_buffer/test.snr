---
- MARCHAL : 
  - SAY : QCONV_STRIP_IN_DATA_BUFFER_TEST 1 K=>1x1
---
- MARCHAL : 
  - SAY : QCONV_STRIP_IN_DATA_BUFFER_TEST 1.00 IN_C=1 IN_W=1 IN_H=1 OUT_C=1
- CSR :
  - WRITE: {ADDR: 32'h00000030, DATA: 32'h00000001} # IN_W
  - WRITE: {ADDR: 32'h00000038, DATA: 32'h00000001} # IN_H
  - WRITE: {ADDR: 32'h00000040, DATA: 32'h00000001} # IN_C
  - WRITE: {ADDR: 32'h00000048, DATA: 32'h00000001} # OUT_W
  - WRITE: {ADDR: 32'h00000050, DATA: 32'h00000001} # OUT_H
  - WRITE: {ADDR: 32'h00000058, DATA: 32'h00000001} # OUT_C
  - WRITE: {ADDR: 32'h00000060, DATA: 32'h00000001} # K_W
  - WRITE: {ADDR: 32'h00000068, DATA: 32'h00000001} # K_H
  - WRITE: {ADDR: 32'h00000070, DATA: 32'h00000000} # PAD_SIZE
  - WRITE: {ADDR: 32'h0000000C, DATA: 32'h14000003} # CTRL[4]=1,CTRL[2]=1,STAT=8'h00,MODE[1:0]=2'b11
  - WAIT : {GPI(0) : 1, TIMEOUT: 10000}             # WAIT for IRQ=1
  - READ : {ADDR: 32'h0000000C, DATA: 32'h04010003} # CTRL[4]=0,CTRL[2]=1,STAT=8'h01,MODE[1:0]=2'b11
  - WRITE: {ADDR: 32'h0000000C, DATA: 32'h00000003} # CTRL[4]=0,CTRL[2]=1,STAT=8'h00,MODE[1:0]=2'b11
  - WAIT : {GPI(0) : 0, TIMEOUT: 10000}             # WAIT for IRQ=0
- I :
  - XFER : {DATA : ["64'h00000000FFFFFFFF"]}
- O :
  - READY: 1
  - WAIT : {VALID: 1}
  - CHECK: {ELEM : [[[["64'h5555555555555555",
                       "64'h----------------",
                       "64'h----------------",
                       "64'h----------------",
                       "64'h----------------"]]]],
            ATRB : {C: [3,0,0,0,0,0,0,0,4], D: [7], X: [7], Y: [7]}}
  - READY: 0
---
- MARCHAL : 
  - SAY : QCONV_STRIP_IN_DATA_BUFFER_TEST 1 DONE
---
- MARCHAL : 
  - SAY : QCONV_STRIP_IN_DATA_BUFFER_TEST 2 K=>3x3
---
- MARCHAL : 
  - SAY : QCONV_STRIP_IN_DATA_BUFFER_TEST 2.00 IN_C=1 IN_W=3 IN_H=3 OUT_C=1
- CSR :
  - WRITE: {ADDR: 32'h00000030, DATA: 32'h00000003} # IN_W
  - WRITE: {ADDR: 32'h00000038, DATA: 32'h00000003} # IN_H
  - WRITE: {ADDR: 32'h00000040, DATA: 32'h00000001} # IN_C
  - WRITE: {ADDR: 32'h00000048, DATA: 32'h00000001} # OUT_W
  - WRITE: {ADDR: 32'h00000050, DATA: 32'h00000001} # OUT_H
  - WRITE: {ADDR: 32'h00000058, DATA: 32'h00000001} # OUT_C
  - WRITE: {ADDR: 32'h00000060, DATA: 32'h00000003} # K_W
  - WRITE: {ADDR: 32'h00000068, DATA: 32'h00000003} # K_H
  - WRITE: {ADDR: 32'h00000070, DATA: 32'h00000000} # PAD_SIZE
  - WRITE: {ADDR: 32'h0000000C, DATA: 32'h14000003} # CTRL[4]=1,CTRL[2]=1,STAT=8'h00,MODE[1:0]=2'b11
  - WAIT : {GPI(0) : 1, TIMEOUT: 10000}             # WAIT for IRQ=1
  - READ : {ADDR: 32'h0000000C, DATA: 32'h04010003} # CTRL[4]=0,CTRL[2]=1,STAT=8'h01,MODE[1:0]=2'b11
  - WRITE: {ADDR: 32'h0000000C, DATA: 32'h00000003} # CTRL[4]=0,CTRL[2]=1,STAT=8'h00,MODE[1:0]=2'b11
  - WAIT : {GPI(0) : 0, TIMEOUT: 10000}             # WAIT for IRQ=0
- I :
  - XFER : {DATA : ["64'h0000000000000001"]}
  - XFER : {DATA : ["64'h0000000100000000"]}
  - XFER : {DATA : ["64'h0000000100000001"]}
  - XFER : {DATA : ["64'h0000000000000002"]}
  - XFER : {DATA : ["64'h0000000200000000"]}
  - XFER : {DATA : ["64'h0000000200000002"]}
  - XFER : {DATA : ["64'h0000000000000004"]}
  - XFER : {DATA : ["64'h0000000400000000"]}
  - XFER : {DATA : ["64'h0000000400000004"]}
- O :
  - READY: 1
  - WAIT : {VALID: 1}
  - CHECK: {ELEM : [[[["64'h0000000000000001",
                       "64'h0000000000000002",
                       "64'h0000000000000003",
                       "64'h0000000000000004",
                       "64'h0000000000000008",
                       "64'h000000000000000C",
                       "64'h0000000000000010",
                       "64'h0000000000000020",
                       "64'h0000000000000030"]]]],
            ATRB : {C: [3,1,1,1,1,1,1,1,5], D: [7], X: [7], Y: [7]}}
  - READY: 0
